{
    "name": "laravel-enso/permissionmanager",
    "description": "Permission Manager dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "permission-manager",
        "laravel-permissions"
    ],
    "homepage": "https://github.com/laravel-enso/PermissionManager",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel-enso/core": "4.0.*",
        "laravel-enso/formbuilder": "3.0.*",
        "laravel-enso/helpers": "1.10.*",
        "laravel-enso/rolemanager": "3.0.*",
        "laravel-enso/structuremanager": "3.0.*",
        "laravel-enso/tutorialmanager": "3.0.*",
        "laravel-enso/vuedatatable": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\PermissionManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\PermissionManager\\AppServiceProvider",
                "LaravelEnso\\PermissionManager\\AuthServiceProvider"
            ],
            "aliases": {}
        }
    }
}
