{
    "name": "laravel-enso/permissionmanager",
    "description": "Permission Manager dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "permission-manager",
        "laravel-permissions"
    ],
    "homepage": "https://github.com/laravel-enso/PermissionManager",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.5.*",
        "laravel-enso/datatable": "1.3.*",
        "laravel-enso/dbsyncmigrations": "1.3.*",
        "laravel-enso/formbuilder": "1.3.*",
        "laravel-enso/helpers": "1.3.*",
        "laravel-enso/rolemanager": "1.3.*",
        "laravel-enso/structuremanager": "1.3.*",
        "laravel-enso/tutorialmanager": "1.3.*"
    },
    "require-dev": {
        "laravel-enso/testhelper": "1.3.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\PermissionManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\PermissionManager\\PermissionsServiceProvider"
            ],
            "aliases": {}
        }
    }
}
